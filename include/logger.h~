#ifndef LOGGER_H
#define LOGGER_H

#include <stdio.h>
#include <stdarg.h>

void log(FILE * stream, const char * format, va_list arg);
void logInfo(const char * format, va_list arg);
void logWarn(const char * format, va_list arg);
void logError(const char * format, va_list arg);

#endif /* LOGGER_H */
